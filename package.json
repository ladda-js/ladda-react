    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.4.1"
  },
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src ./node_modules/.bin/mocha --compilers js:babel-register --reporter spec src/**/*.spec.js --require mocha.config",
