  "dependencies": {},
  "peerDependencies": {
    "react": "^15.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
